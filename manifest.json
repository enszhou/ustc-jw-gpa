{
    "name": "GPA",
    "manifest_version": 2,
    "version": "2.1",
    "description": "calculate your gpa",
    "icons": {
        "16": "image/icon.png",
        "19": "image/icon.png",
        "48": "image/icon.png",
        "128": "image/icon.png"
    },
    "page_action": {
        "default_icon": "image/icon.png", 
        "default_title": "点我退学"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://jw.ustc.edu.cn/eams5/home"
            ],
            "js": [
                "js/jquery-3.4.1.min.js",
                "js/content2.0.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "*://jw.ustc.edu.cn/eams5/home",
        "webRequest",
        "webRequestBlocking",
        "tabs"
    ]
}